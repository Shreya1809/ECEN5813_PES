static void test_circbuf_wrap_add(void **state){
    //cb_struct my_cb;
    cb_struct *ptr = (cb_struct*)malloc(sizeof(cb_struct));
    int8_t j;
    cb_enum cb_status = cb_init(ptr,BUFFER_LENGTH);
    assert_int_equal(cb_status, CB_SUCCESS);
